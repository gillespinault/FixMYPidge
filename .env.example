# Example environment variables for FixMyPidge
# Copy this file to .env.local and fill in your actual values

# Application
NODE_ENV=development
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Supabase Configuration (use local CLI stack)
NEXT_PUBLIC_SUPABASE_URL=http://127.0.0.1:54321
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# n8n Integration
NEXT_PUBLIC_N8N_WEBHOOK_URL=https://n8n.robotsinlove.be/webhook/fixmypidge
N8N_WEBHOOK_SECRET=your_webhook_secret_here

# Maps (optional - for future features)
NEXT_PUBLIC_MAPBOX_API_TOKEN=your_mapbox_token_here

# Disable Next.js telemetry
NEXT_TELEMETRY_DISABLED=1